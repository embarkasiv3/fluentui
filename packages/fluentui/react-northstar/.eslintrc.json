{
  "extends": ["plugin:@fluentui/eslint-plugin/react-northstar"],
  "ignorePatterns": ["src/themes/teams/components/Icon/svg/ProcessedIcons"],
  "overrides": [
    {
      "files": "**/icons/*.tsx",
      "rules": {
        "react/prop-types": "off"
      }
    },
    {
      "files": "src/**/*.{ts,tsx}",
      "globals": {
        "document": "off",
        "window": "off"
      },
      "rules": {
        "no-undef": "error"
      }
    }
  ],
  "root": true
}
